{% if page.url == '/all/' %}
<script src="{{ '/taglist.js' | jsHash }}"></script>
{% endif %}
{% if (page.filePathStem | startsWith('/posts/')) %}
<script src="{{ '/toc.js' | jsHash }}"></script>
{% endif %}

{% if page.url != '/all/' %}
<script src="/ql.v2.1.0.js"></script>
<script>
  quicklink.listen({
    throttle: 3,
    ignores: [/\/all\/\?tag=/, /feed\.xml/]
  })
</script>
{% endif %}

